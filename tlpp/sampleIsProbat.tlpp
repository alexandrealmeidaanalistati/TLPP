#include 'tlpp-core.th'


user function sampleIsProbat()
 
  local jModule
 
  if ( tlpp.module( 'PROBAT', @jModule ) )
    FWAlertInfo( 'Ferramenta: ' + jModule['module'] )
    FWAlertInfo( 'Instalado?: ' + if( jModule['linked'], 'sim', 'não' ) )
    FWAlertInfo( 'Versão....: ' + jModule['version'] )
  else
    FWAlertInfo( ' >> Ferramenta não instalada <<' )
  endif
 
return Nil
